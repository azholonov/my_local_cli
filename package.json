{
  "name": "my-local-ai",
  "version": "0.1.0",
  "description": "A full-featured AI agent CLI with multi-provider support",
  "type": "module",
  "bin": {
    "my-local-ai": "./bin/cli.ts"
  },
  "scripts": {
    "dev": "tsx bin/cli.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "ai",
    "cli",
    "agent",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@types/node": "^25.3.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "diff": "^8.0.3",
    "dotenv": "^17.3.1",
    "glob": "^13.0.6",
    "gpt-tokenizer": "^3.4.0",
    "ink": "^5.2.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "ollama": "^0.6.3",
    "openai": "^6.22.0",
    "react": "^18.3.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/react": "^19.2.14",
    "vitest": "^4.0.18"
  }
}
